@page
@model BirdPlatFormApp.Pages.SchedulePages.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].ScheduleDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].AvailableSlots)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].CreatedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].DeletedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].DeleteBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule[0].BirdService)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Schedule) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvailableSlots)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeletedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeleteBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BirdService.ProductName)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
